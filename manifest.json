{
  "manifest_version": 3,
  "name": "REO Events",
  "version": "1.0",
  "description": "Lists REO's AB test Events",
  "permissions": [
    "activeTab",
    "declarativeNetRequest",
    "tabs",
    "storage",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "devtools_page": "devtools.html",
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}