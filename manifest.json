{
  "manifest_version": 3,
  "name": "REO Events",
  "version": "1.0",
  "description": "Lists REO's AB test Events",
  "permissions": [
    "activeTab",
    "webRequest",
    "tabs",
    "storage",
    "webNavigation"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "devtools_page": "devtools.html",
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["style.css"],
      "matches": ["<all_urls>"]
    }
  ]
}
